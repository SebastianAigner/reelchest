{
  "name": "parcel-typescript-react-tailwind",
  "version": "0.2.1",
  "main": "index.html",
  "license": "MIT",
  "description": "Trying-out Tailwind CSS with Parcel",
  "scripts": {
    "start": "node dev-proxy/server.js",
    "build": "esbuild src/main.ts --bundle --outdir=build '--define:process.env.NODE_ENV=\"development\"' && cp src/index.html build/",
    "clean": "rm -rf .cache dist",
    "build-dev": "NODE_ENV=development parcel build src/index.html --no-minify"
  },
  "devDependencies": {
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "express": "^4.17.2",
    "http-proxy-middleware": "^2.0.6",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/react-editext": "^3.1.0",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.26.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-editext": "^3.17.2",
    "react-lazy-load-image-component": "^1.5.1",
    "react-router-dom": "^5.2.0",
    "swr": "^0.4.2"
  }
}
